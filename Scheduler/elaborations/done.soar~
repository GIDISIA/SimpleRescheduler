sp {Scheduler*elaborate*done
   (state <s> ^name Scheduler
              ^calculations.pending-general-calculations 0
              ^initTardiness <initTard>
              ^totTard <tottard> < <initTard>)
-->
   (halt)
}

sp {Scheduler*elaborate*done*b
   (state <s> ^name Scheduler
              ^calculations <c>)
   (<c> ^pending-general-calculations 0
        ^step 400)
-->
   (halt)
}
